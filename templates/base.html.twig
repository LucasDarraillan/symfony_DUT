<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        {% endblock %}
    </head>
    <body>

        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                ENTETE
                </div>
            </div>
            <div class="row">
                <div class="col-md-3">
                    <nav class="navbar navbar-light bg-light">
                        <div class="container-fluid">
                            <a class="navbar-brand" href="#">
                            <img src="https://th.bing.com/th/id/R.28c89e9126a8c57aabdbf12f1d6c185a?rik=WyG9FnajKyk2vA&riu=http%3a%2f%2fpngimg.com%2fuploads%2fletter_p%2fletter_p_PNG112.png&ehk=sIAOfLnS4sX5x2MbcFCMowpjKo62vnaP0bkG%2bjJcGo8%3d&risl=&pid=ImgRaw&r=0" alt="" width="30" height="24" class="d-inline-block align-text-top">
                            ProStages
                            </a>
                        </div>
                    </nav>
                </div>
                <div class="col-md-9">
                {% block body %}{% endblock %}
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                PIED DE PAGE
                </div>
            </div>
        </div>

    </body>
</html>

